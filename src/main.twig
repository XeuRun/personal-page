<!DOCTYPE HTML>
<html>
<head>
  <title>{{ me.name ~ ' ' ~ me.surname }}</title>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1"/>
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
  <link rel="manifest" href="/site.webmanifest">
  <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#9f00a7">
  <meta name="theme-color" content="#ffffff">
  <link rel="stylesheet" href="{{ env.path.assets }}/app.css"/>
  <link rel="stylesheet" href="{{ env.path.assets }}/vendor/angular/angular-csp.css"/>
  {%- block stylesheets -%}{% endblock %}
  <script src="{{ env.path.assets }}/vendor/angular/angular.min.js"></script>
  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-124541017-1"></script>
  <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-124541017-1');

  </script>
</head>
<body class="loading" ng-app="app">
<div id="wrapper">
  <main>
    {%- block body %}
      {% include './components/social/social.html.twig' %}
      {% include './components/profile/profile.html.twig' %}
      {% include './components/portfolio/portfolio.html.twig' %}
    {% endblock -%}
  </main>
  <footer id="footer">
    <ul class="copyright">
      <li class="me">&copy; {{ me.name ~ ' ' ~ me.surname }}</li>
    </ul>
  </footer>
</div>
<script src="{{ env.path.assets }}/app.js"></script>
<script src="{{ env.path.assets }}/common/common.js"></script>
<script src="{{ env.path.assets }}/common/js/providers/FileProvider.js"></script>
<script src="{{ env.path.assets }}/common/js/providers/FakeProvider.js"></script>
<script src="{{ env.path.assets }}/common/js/services/CrawlerService.js"></script>
<script src="{{ env.path.assets }}/social/js/controllers/SocialController.js"></script>
<script src="{{ env.path.assets }}/profile/js/controllers/ProfileController.js"></script>
<script src="{{ env.path.assets }}/portfolio/js/controllers/PortfolioController.js"></script>
{%- block javascripts -%}{%- endblock %}
</body>
</html>
